void loop(void)
{
  
  matrix.fillScreen(0);
  matrix.setCursor(0, 0);
  
  

  
    bool down = Touch_getXY();
    if(down) {
      Serial.print("x: ");
      Serial.print(pixel_x);
      Serial.print("\n");
      Serial.print("Y: ");
      Serial.print(pixel_y);
      Serial.print("\n");
      
    }
   


    //button 01
    btn1.press(down && btn1.contains(pixel_x, pixel_y)); 
    if (btn1.justPressed())  {
      if (light1 == false) {
        light1 = true;
        btn1.initButton(&tft,  START             , START, BTN_WIDTH, BTN_HEIGHT, WHITE, BLACK, YELLOW,"1" , 2);  
      }
      else {
         btn1.initButton(&tft,  START             , START, BTN_WIDTH, BTN_HEIGHT, WHITE, BLACK, CYAN,"1" , 2);
         light1 = false;
      }  
      btn1.drawButton(true);
    }
    if (light1 == true) {
      matrix.drawPixel(0,0, 0xFFE0); 
    }
    

    //button 02
    btn2.press(down && btn2.contains(pixel_x, pixel_y)); 
    if (btn2.justReleased()) { 
      if (light2 == false){
        light2 = true;
        btn2.initButton(&tft,  START + WIDTH * 1, START, BTN_WIDTH, BTN_HEIGHT, WHITE, CYAN, BLACK,"2" , 2); 
      btn2.drawButton();
      }
      else {
        light2 = false;
      }    
      // delay(250);
    }
    if (light2 == true) {
      matrix.drawPixel(1,0, 244);  
        btn2.initButton(&tft,  START + WIDTH * 1, START, BTN_WIDTH, BTN_HEIGHT, WHITE, YELLOW, BLACK,"2" , 2);
    }
    if (btn2.justPressed())  {
      // btn2.drawButton(true);
    }

    //button 03
   btn3.press(down && btn3.contains(pixel_x, pixel_y)); 
    if (btn3.justReleased()) { 
      if (light3 == false){
        light3 = true;
        btn3.initButton(&tft,  START + WIDTH * 2, START, BTN_WIDTH, BTN_HEIGHT, WHITE, CYAN, BLACK,"3" , 2); 
      btn3.drawButton();
      }
      else {
        light3 = false;
      }    
      // delay(250);
    }
    if (light3 == true) {
      matrix.drawPixel(2,0, 244);  
        btn3.initButton(&tft,  START + WIDTH * 2, START, BTN_WIDTH, BTN_HEIGHT, WHITE, YELLOW, BLACK,"3" , 2);
    }
    if (btn3.justPressed())  {
      // btn3.drawButton(true);
    }

    //button 04
   btn4.press(down && btn4.contains(pixel_x, pixel_y)); 
    if (btn4.justReleased()) { 
      if (light4 == false){
        light4 = true;
        btn4.initButton(&tft, START + WIDTH * 3, START, BTN_WIDTH, BTN_HEIGHT, WHITE, CYAN, BLACK,"4" , 2); 
      btn4.drawButton();
      }
      else {
        light4 = false;
      }    
      // delay(250);
    }
    if (light4 == true) {
      matrix.drawPixel(3,0, 244);  
        btn4.initButton(&tft,  START + WIDTH * 3, START, BTN_WIDTH, BTN_HEIGHT, WHITE, YELLOW, BLACK,"4" , 2);
    }
    if (btn4.justPressed())  {
      // btn4.drawButton(true);
    }

