    btn5.press(down && btn5.contains(pixel_x, pixel_y)); 
    if (btn5.justReleased()) { 
      if (light5 == false){
        light5 = true;
        btn5.initButton(&tft, START + WIDTH * 4, START, BTN_WIDTH, BTN_HEIGHT, WHITE, CYAN, BLACK,"5" , 2); 
